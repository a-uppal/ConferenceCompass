{
  "expo": {
    "name": "Conference Compass",
    "slug": "conference-compass",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "conference-compass",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.datacompass.conferencecompass",
      "infoPlist": {
        "NSCameraUsageDescription": "Conference Compass needs camera access to capture badge photos and session slides.",
        "NSMicrophoneUsageDescription": "Conference Compass needs microphone access to record voice notes.",
        "NSPhotoLibraryUsageDescription": "Conference Compass needs photo library access to save and select photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0F172A"
      },
      "package": "com.datacompass.conferencecompass",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-camera",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#0D9488"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a5b51865-e03c-4dad-9383-9c0327265dfe"
      }
    },
    "owner": "auppal",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a5b51865-e03c-4dad-9383-9c0327265dfe"
    }
  }
}
